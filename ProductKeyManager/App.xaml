<Application x:Class="ProductKeyManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProductKeyManager"
             StartupUri="Views/MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles/Icons.ProductKeyManager.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles/Styles.App.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles/Styles.Text.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles/Styles.ToolBar.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>